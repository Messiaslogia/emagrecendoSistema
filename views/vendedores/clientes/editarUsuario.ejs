                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <!-- End Navbar -->

        <div class="row justify-content-center">
            <div class="col-md-10 bg-light mt-4">
                <!-- Default form register -->
                <form class="text-center border border-light p-5" action="/vendedores/edit/<%= id %>?user=<%= idCripted %>" method="post" enctype="application/x-www-form-urlencoded">
                    <input type="text" style="display: none;" value="<%= id %>" id="idUserEdit">
                    <p class="h4 mb-4">Editar Usuário</p>
                    
                    <section>
                        <h5 class="text-dark">Informações Pessoais</h5>
                    
                        <div class="form-row mb-4">
                            <div class="col text-start">
                                <!-- First name -->
                                <label for="Password_input" class="text-start fs-4">Nome:</label>
                                <input id="Name_input" name="nome" type="text" class="form-control" placeholder="Nome completo">
                            </div>
                        </div>

                        <!-- E-mail -->
                        <div class="text-start">
                            <label for="Email_input" class="text-start fs-4">Email:</label>
                            <input id="Email_input" name="email" type="email" class="form-control mb-4" placeholder="E-mail">
                        </div>
                        
                        
                        <!-- Password -->
                        <div class="text-start">
                            <label for="Password_input" class="text-start fs-4">Senha:</label>
                            <input id="Password_input" name="senha" type="text" class="form-control mb-4" placeholder="Senha"
                            aria-describedby="defaultRegisterFormPasswordHelpBlock">
                        </div>

                        <!-- Phone number -->
                        <div class="text-start">
                            <label for="Telefone_input" class="text-start fs-4">Telefone:</label>
                            <input id="Telefone_input" name="telefone" type="text" class="form-control mb-4" placeholder="Whatsapp"
                            aria-describedby="defaultRegisterFormPhoneHelpBlock">
                        </div>

                        <!-- CPF -->
                        <div class="text-start">
                            <label for="CPF_input" class="text-start fs-4">CPF:</label>
                            <input id="CPF_input" name="cpf" type="text" class="form-control mb-4" placeholder="CPF"
                            aria-describedby="defaultRegisterFormPhoneHelpBlock">
                        </div>

                        <!-- Nascimento -->
                        <div class="text-start">
                            <label for="Nascimento" class="text-start fs-4">Data de Nascimento:</label>
                            <input id="Nascimento_input" name="nascimento" type="text" class="form-control mb-4" placeholder="Nascimento"
                            aria-describedby="defaultRegisterFormPhoneHelpBlock">
                        </div>
                    
                        <hr>
                    </section>
                    
                    <section>
                        <h5 class="text-dark">Endereço</h5>
                    
                        <input id="Cep_input" name="cep" type="text" class="form-control mb-4" placeholder="Cep*" required>
                    
                        <input id="Endereco_input" name="endereco" type="text" class="form-control mb-4" placeholder="Endereço do Usuário*"
                            required>
                    
                        <div class="container text-center">
                            <div class="row">
                                <div class="col">
                                    <input id="Bairro_input" name="bairro" type="text" class="form-control mb-4" placeholder="Bairro*"
                                        required>
                                </div>
                    
                                <div class="col">
                                    <input id="Numero_input" name="numerodoendereco" type="text" class="form-control mb-4"
                                        placeholder="Numero*" required>
                                </div>
                            </div>
                        </div>
                    
                        <!-- Região -->
                        <div>
                            <select id="Regiao_input" name="regiao" class="form-control mb-4" id="exampleFormControlSelect1">
                                <option>Norte</option>
                                <option>Nordeste</option>
                                <option>Centro-Oeste</option>
                                <option>Sudeste</option>
                                <option>Sul</option>
                            </select>
                        </div>
                    
                        <hr>
                    </section>
                    
                    <section>
                        <h5>Redes Sociais</h5>
                        <!-- URl Instagram -->
                        <input id="Instagram_input" name="instagram" type="text" class="form-control mb-4" placeholder="URl Instagram">
                    
                        <!-- URL Facebooks -->
                        <input id="Facebooks_input" name="facebooks" type="text" class="form-control mb-4" placeholder="URL Facebooks">
                    </section>
                    
                        <!-- Sign up button -->
                        <button id="Bt_submit" class="btn btn btn-success my-4 btn-block" type="submit">Cadastrar</button>
                    </form>
                <!-- Default form register -->
            </div>
        </div>

        <a href="/vendedores/clientes?user=<%= idCripted %>" class="ms-3">
            <button type="button" class="btn btn-primary btn-l mt-5" style="background-color: #e91e63;">Voltar</button>
        </a>

        <footer class="footer py-4  ">
            <div class="container-fluid">
                <div class="row align-items-center justify-content-lg-between">
                    <div class="col-lg-4 mb-lg-0 mb-4">
                        <div class="copyright text-center text-sm text-muted text-lg-start">
                            ©
                            <script>
                                document.write(new Date().getFullYear())
                            </script>,
                            <i class="fa fa-heart"></i>
        
                            Emagrecendo
                                com Ela
                        </div>
                    </div>
                </div>
            </div>
        </footer>
