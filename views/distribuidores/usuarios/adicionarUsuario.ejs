        <div class="row justify-content-center">
            <div class="col-md-10 bg-light mt-4">
                <!-- Default form register -->
                <form id="MyForm" class="text-center border border-light p-5" action="/api/novoUsuarioParaDistribuidor?user=<%= idCripted %>" method="post" enctype="application/x-www-form-urlencoded">
                
                    <p class="h4 mb-4">Novo Usuário</p>
                        <input id="idDistribuidor" name="distribuidor" type="text" style="display: none;" class="form-control" value="<%= idDoDistribuidor %>" required>
                    
                    <!-- Informações Pessoais -->
                    <section>
                        <div class="form-row mb-4">
                            <div class="col">
                                <input id="Name_input" name="nome" type="text" id="defaultRegisterFormFirstName" class="form-control"
                                    placeholder="Nome completo" required>
                            </div>
                        </div>
                    
                        <div>
                            <select id="Funcao_input" name="funcao" class="form-control mb-4" required>
                                <option disabled selected value="">Selecione a função</option>
                                <option value="6">Representante</option>
                                <option value="7">Afiliado</option>
                            </select>
                        </div>
                    
                        <input id="Email_input" name="email" type="email" id="defaultRegisterFormEmail" class="form-control mb-4"
                            placeholder="E-mail" required>
                    
                    
                        <input id="Nascimento_input" name="nascimento" type="text" class="form-control mb-4"
                            placeholder="Data de nascimento do Usuário*" required>
                    
                        <input id="CPF_input" name="cpf" type="text" class="form-control mb-4" placeholder="CPF" required>
                    
                        <input id="Telefone_input" name="telefone" type="text" class="form-control mb-4" placeholder="Whatsapp">
                        <hr>
                    </section>
                
                    <!-- Endereço -->
                    <section>
                        <h5 class="text-dark">Endereço</h5>
                    
                        <input id="Cep_input" name="cep" type="text" class="form-control mb-4" placeholder="Cep*" required>
                    
                        <input id="Endereco_input" name="endereco" type="text" class="form-control mb-4" placeholder="Endereço do Usuário*"
                            required>
                    
                        <div class="container text-center">
                            <div class="row">
                                <div class="col">
                                    <input id="Bairro_input" name="bairro" type="text" class="form-control mb-4" placeholder="Bairro*"
                                        required>
                                </div>
                    
                                <div class="col">
                                    <input id="Numero_input" name="numerodoendereco" type="text" class="form-control mb-4"
                                        placeholder="Numero*" required>
                                </div>
                            </div>
                        </div>
                    
                        <div>
                            <select id="Regiao_input" name="regiao" class="form-control mb-4" required>
                                <option disabled selected value="">Selecione a Região</option>
                                <option>Norte</option>
                                <option>Nordeste</option>
                                <option>Centro-Oeste</option>
                                <option>Sudeste</option>
                                <option>Sul</option>
                            </select>
                        </div>
                        <hr>
                    </section>
                    
                    <!-- Redes Sociais -->
                    <section>
                        <h5 class="text-dark">Redes Sociais</h5>
                    
                        <input id="Instagram_input" name="instagram" type="text" class="form-control mb-4" placeholder="URl Instagram">
                    
                        <input id="Facebooks_input" name="facebooks" type="text" class="form-control mb-4" placeholder="URL Facebooks">
                    </section>
                   
                    <!-- Sign up button -->
                    <button id="Bt_submit" class="btn btn btn-success my-4 btn-block" type="submit">Cadastrar</button>            
                </form>
                <!-- Default form register -->
            </div>
        </div>

